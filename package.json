{
  "name": "cross-sourcerer",
  "version": "1.0.0",
  "license": "MIT",
  "main": "main/index.js",
  "productName": "ElectronTypescriptNext",
  "scripts": {
    "clean": "rimraf dist main renderer/out renderer/.next",
    "web": "next ./renderer",
    "desktop": "yarn build-electron && electron .",
    "dev": "next ./renderer",
    "build-renderer": "next build renderer && next export renderer",
    "build-electron": "tsc -p electron-src",
    "build": "yarn build-renderer && yarn build-electron",
    "pack-app": "yarn build && electron-builder --dir",
    "dist": "yarn build && electron-builder",
    "format": "yarn prettier --write '**/*.ts*' --loglevel silent",
    "test": "yarn test:check && yarn test:elint",
    "type-check": "yarn tsc"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "@tailwindcss/ui": "^0.7.2",
    "electron-is-dev": "^1.1.0",
    "electron-next": "^3.1.5",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "^10.2.0",
    "next-auth": "^3.22.0",
    "next-images": "^1.7.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^4.2.0",
    "tailwindcss": "^2.1.2",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.169",
    "@types/moment": "^2.13.0",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.9",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "autoprefixer": "^10.2.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "postcss": "^8.2.15",
    "postcss-cli": "^8.3.1",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.0",
    "styled-jsx": "^3.4.4",
    "styled-jsx-plugin-sass": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ]
  }
}
